@{
	Layout = null;
}
@model IEnumerable<LinkModel>

<aside class="widget">
	<h2 class="widget-title">Link List </h2>
	<div class="widget-text">
		<ul class="widget-link-list">
			@foreach (var item in Model)
			{
				<li><a href="@item.Url" title="@item.Title" target="_blank">@item.Title</a></li>
			}
		</ul>
	</div>
</aside>
